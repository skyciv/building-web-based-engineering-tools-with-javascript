{
	"meta": {
		"name": "Simply Supported Beam Analysis",
		"short_description": "Simple Supported Beam Analysis Software to calculate reactions, bending, shear and deflection results.",
		"long_description": "Simply Supported Beam Analysis that takes in inputs for a distributed load on a simply supported beam. Allows users to export a simplified report with a summary of results and diagrams for bending moment, shear force and deflection if I and E are inputed.",
		"tags": "draft, beam, calculator",
		"category": "analysis",
		"access": "public",
		"default_unit_system": "metric",
		"contact": {
			"name": "Sam Carigliano",
			"role": "",
			"email": "sam@skyciv.com",
			"company": "SkyCiv Engineering",
			"logo": "https://skyciv.com/media/logos/logo-pack/SkyCiv_Logo_Dark_Poweredby.png"
		},
		"related": {
			"title": "<i class='calculator icon'></i>",
			"Column Analysis Calculator": {
				"uid": "1007-column-calculator"
			},
			"2D Stiffness Method Analysisr": {
				"uid": "1006-stiffness-method-calculator"
			}	
		}
	},
	"input_variables": {
		"ui_div": {
			"type": "div",
			"id": "ui-div",
			"qd_center": true, 
			"allow_graphic_report": false
	
		},
		"sub_heading1": {
			"type": "heading",
			"label": "Arrangement"
		},
		"L": {
			"type": "number",
			"label": "Beam Span, L",
			"symbol": "L",
			"units": "mm",
			"info": "Length of your Beam",
			"default": 1200,
			"min": 10
		},
		"support_a": {
			"label": "Support at A",
			"symbol": "S_{A}",
			"info": "Support type at end A",
			"default": "Fixed",
			"type": "dropdown",
			"options": [
				{
					"name": "Fixed (FFFFFF)",
					"value": "Fixed"
				},
				{
					"selected": true,
					"name": "Pinned (FFFFFR)",
					"value": "Pinned"
				},
				{
					"name": "Roller (RFFRRR)",
					"value": "Roller"
				},
				{
					"name": "Cantilever",
					"value": "Cantilever"
				}
			]
		},
		"support_b": {
			"label": "Support at B",
			"symbol": "S_{B}",
			"info": "Support type at end B",
			"default": "",
			"type": "dropdown",
			"nullable": true,
			"options": [
				{
					"name": "Fixed (FFFFFF)",
					"value": "Fixed"
				},
				{
					"name": "Pinned (FFFFFR)",
					"value": "Pinned"
				},
				{
					"name": "Roller (RFFRRR)",
					"value": "Roller",
					"selected": true
				},
				{
					"name": "Cantilever",
					"value": "Cantilever"
				}
			]
		},
		"sub_heading2": {
			"type": "heading",
			"label": "Loads"
		},
		"DL_mag": {
			"type": "number",
			"label": "Distributed Load Magnitude",
			"symbol": "D_{L}",
			"units": "kN/m",
			"info": "The uniform distributed load magnitude on your beam",
			"default": 1.25,
			"min": 0,
			"max": 400,
			"step": 1,
			"nullable": true
		},
		"PL_mag": {
			"type": "number",
			"label": "Point Load Magnitude",
			"symbol": "D_{L}",
			"units": "kN",
			"info": "Magnitude of the point load",
			"default": 2,
			"min": 0,
			"step": 1,
			"nullable": true
		},
		"PL_dist": {
			"type": "number",
			"label": "Point Load Distance, d",
			"symbol": "PL_{d}",
			"units": "mm",
			"info": "Distance of point load from the start of the member",
			"default": 250,
			"min": 0,
			"step": 1,
			"nullable": true
		},
		"calculate_deflection": {
			"type": "checkbox",
			"symbol": "D_{y}",
			"is_locked": true,
			"label": "Calculate Deflection",
			"default": false,
			"visible_variables": {
				"false": [
					[
						"hide",
						"E"
					],
					[
						"hide",
						"I_z"
					]
				],
				"true": [
					[
						"show",
						"E"
					],
					[
						"show",
						"I_z"
					]
				]
			}
		},
		"E": {
			"type": "number",
			"label": "Young's Modulus (E)",
			"symbol": "E",
			"units": "MPa",
			"info": "Young's Modulus of your material",
			"default": 200000,
			"min": 0,
			"step": 1000,
			"nullable": true
		},
		"I_z": {
			"type": "number",
			"label": "Moment of Inertia about Z",
			"symbol": "I_z",
			"units": "mm^4",
			"info": "Moment of Inertia about Z Axis",
			"default": "2.2e8",
			"min": 0,
			"step": 1000,
			"nullable": true
		}
	}
}