{
	"meta": {
		"name": "Build Your Own Calculator",
		"short_description": "Insert short description.",
		"tags": "calculator",
		"category": "analysis",
		"access": "private",
		"default_unit_system": "metric",
		"contact": {
			"name": "Insert Name",
			"role": "",
			"email": "Insert Email",
			"company": "Insert Company",
			"logo": "https://skyciv.com/media/logos/logo-pack/SkyCiv_Logo_Dark_Poweredby.png"
		}
	},
	"input_variables": {
		"Section_Heading": {
			"type": "heading",
			"label": "Section Input"
		},
		"input_div": {
			"type": "div",
			"id": "input-div",
			"allow_graphic_report": false
		},
		"centre_div": {
			"type": "div",
			"id": "centre-div",
			"qd_center": true, 
			"allow_graphic_report": false
		},
		"dimensions_subheading": {
			"type": "subheading",
			"label": "Dimensions"
		},
		"height": {
			"type": "number",
			"integer": false,
			"label": "Height",
			"symbol": "H",
			"units": "mm",
			"info": "Total section height.",
			"greater_than": "flange_thickness",
			"default": 250,
			"step": 1,
			"min": 100,
			"max": 1000
		},
		"width": {
			"type": "number",
			"integer": false,
			"label": "Width",
			"symbol": "B",
			"units": "mm",
			"info": "Total section width.",
			"greater_than": "web_thickness",
			"default": 100,
			"step": 1,
			"min": 50,
			"max": 600
		},
		"thickness_subheading": {
			"type": "subheading",
			"label": "Thickness"
		},
		"flange_thickness": {
			"type": "number",
			"integer": false,
			"label": "Flange Thickness",
			"symbol": "t_f",
			"units": "mm",
			"info": "Thickness of section flange.",
			"default": 10,
			"step": 1,
			"min": 1,
			"max": 30
		},
		"web_thickness": {
			"type": "number",
			"integer": false,
			"label": "Web Thickness",
			"symbol": "t_w",
			"units": "mm",
			"info": "Thickness of section web.",
			"default": 5,
			"step": 1,
			"min": 1,
			"max": 20
		},
		"graphics_subheading": {
			"type": "subheading",
			"label": "Section Graphics"
		},
		"show_axis": {
			"type": "checkbox",
			"symbol": "Axis",
			"label": "Show Axis",
			"highlight_row": true,
			"default": "checked",
			"visible_variables": {
				"checked" : [
					["show","vertical_axis_colour"],
					["show","horizontal_axis_colour"],
					["show","axis_size"]
					],
				"unchecked" : [
					["hide","vertical_axis_colour"],
					["hide","horizontal_axis_colour"],
					["hide","axis_size"]
					]
			}
		},
		"vertical_axis_colour": {
			"type": "dropdown",
			"label": "Vertical Axis Colour",
			"symbol": "Vertical \\:Axis",
			"info": "Colour of vertical axis in section diagram.",
			"nullable": false,
			"options": [
				{
					"selected": true,
					"name": "Green",
					"value": "green"
				},
				{
					"name": "Red",
					"value": "red"
				},
				{
					"name": "Blue",
					"value": "blue"
				}
			]
		},
		"horizontal_axis_colour": {
			"type": "dropdown",
			"label": "Horizontal Axis Colour",
			"symbol": "Horizontal \\:Axis",
			"info": "Colour of horizontal axis in section diagram.",
			"nullable": false,
			"options": [
				{
					"name": "Green",
					"value": "green"
				},
				{
					"name": "Red",
					"value": "red"
				},
				{
					"selected": true,
					"name": "Blue",
					"value": "blue"
				}
			]
		},
		"axis_size": {
			"type": "dropdown",
			"label": "Axis Size",
			"symbol": "Axis \\; Size",
			"info": "Size of axis in section diagram.",
			"nullable": false,
			"options": [
				{
					"name": "Small",
					"value": "small"
				},
				{
					"selected": true,
					"name": "Medium",
					"value": "medium"
				},
				{
					"name": "Large",
					"value": "large"
				}
			]
		},
		"Table_Heading": {
			"type": "heading",
			"label": "Input Table"
		},
		"input_table": {
            "type": "table",
            "label": "Input Table",
            "button_text": "Open Table",
			"min_rows": 2,
            "default_data":[
                ["1", "A", 10, 7, "Beam", "option_1"],
                ["2", "B", 20, 15, "Column", "option_3"],
				["3", "C", 30, 18, "Slab", "option_2"],
				["4", "D", 40, 25, "Footing", "option_3"],
				["5", "E", 50, 55, "Wall", "option_2"]
            ],
            "exclude_from_input_table": true,
            "info": "Spreadsheet-style input table.",
            "table": {
                "name": "My Input Table",
                "notes": "",
                "version": 1,
                "selector": "#vertical-line-entry-table",
				"selectable": true,
                "auto_new_row_on_enter_key": true,
                "auto_validate ": true,
                "remove_buttons": false,
                "columns": [
					{
                        "id": "num_id",
                        "title": "Numerical ID",
						"tooltip": "Auto-incrementing numerical ID column.",
                        "type": "integer",
						"auto_increment_id": true,
                        "cell_type": "input_text",
                        "cell_width": "auto"
                    },
					{
                        "id": "alph_id",
                        "title": "Alphabetical ID",
                        "tooltip": "Auto-incrementing alphabetical ID column.",
						"default_value": "A",
                        "type": "string",
                        "cell_type": "input_text",
                        "cell_width": "auto",
						"increment": "alphabetical",
						"nullable": true
                    },
                    {
                        "id": "custom_increment",
                        "title": "Custom Increment",
                        "tooltip": "New rows auto-increment by 10 compared to the previous row.",
                        "default_value": 0,
                        "minimum": 0,
                        "type": "number",
                        "cell_type": "input_text",
                        "cell_width": "auto",
						"increment_new": 10,
						"unique": true,
                        "disabled": false
                    },
                    {
                        "id": "number_input",
                        "title": "Number Input",
                        "default_value": 0,
                        "maximum": 0,
                        "type": "number",
                        "cell_type": "input_text",
                        "cell_width": "auto",
						"decimal_places": 3
                    },
					{
                        "id": "text_input",
                        "title": "Text Input",
						"default_value": "Insert Text",
                        "type": "string",
                        "cell_type": "input_text",
                        "cell_width": "auto",
						"nullable": true
                    },
					{
						"id": "dropdown_input",
						"title": "Dropdown Input",
						"default_value": "1",
						"type": "string",
						"cell_type": "dropdown",
						"cell_dropdown_values": ["1", "2", "3"],
						"cell_dropdown_labels": ["Option 1", "Option 2", "Option 3"],
						"cell_width": "auto"
					}
                ]
            }
        },
		"Other_Inputs": {
			"type": "heading",
			"label": "Other Inputs"
		},
		"plain_text_input": {
			"type": "text",
			"label": "Text Input",
			"symbol": "Text",
			"default": "Input Text"
		},
		"combined_input_1": {
			"type": "text",
			"label": "Combined Input 1",
			"symbol": "Combined \\;1",
			"default": "First",
			"display_units": "1",
			"combine_with": "combined_input_2",
			"combined_label": "Combined Inputs"
		},
		"combined_input_2": {
			"type": "text",
			"label": "Combined Input 2",
			"symbol": "Combined \\;2",
			"display_units": "2",
			"default": "Second"
		},
		"radio_input": {
			"type": "radio",
			"label": "Radio Input",
			"symbol": "Radio",
			"default": "option_1",
			"options": [
				{
					"name": "Option 1",
					"value": "option_1"
				},
				{
					"name": "Option 2",
					"value": "option_2"
				}
			]
		}
	}
}