{
	"meta": {
		"name": "Building Web-Based Engineering Tools",
		"short_description": "Simple Supported Beam Analysis Software to calculate reactions, bending, shear and deflection results.",
		"tags": "beam, calculator",
		"category": "analysis",
		"access": "private",
		"default_unit_system": "metric",
		"contact": {
			"name": "George Chard",
			"role": "",
			"email": "George.Chard@skyciv.com",
			"company": "SkyCiv Engineering",
			"logo": "https://skyciv.com/media/logos/logo-pack/SkyCiv_Logo_Dark_Poweredby.png"
		}
	},
	"input_variables": {
		"ui_div": {
			"type": "div",
			"id": "ui-div",
			"qd_center": true, 
			"allow_graphic_report": false
		},
		"Arrangement_Heading": {
			"type": "heading",
			"label": "Arrangement"
		},
		"L": {
			"type": "number",
			"integer": true,
			"label": "Beam Span (L)",
			"symbol": "L",
			"units": "mm",
			"info": "Length of your Beam",
			"default": 6000,
			"min": 10,
			"max": 10000
		},
		"Loading_Heading": {
			"type": "heading",
			"label": "Loading"
		},
		"w": {
			"type": "dropdown",
			"label": "Distributed Load",
			"symbol": "w",
			"units": "kN",
			"info": "Uniform distributed load on your beam",
			"options": [
				{
					"name": "No Force",
					"value": 0
				},
				{
					"name": 1,
					"value": 1
				},
				{
					"name": 2,
					"value": 2,
					"selected": true
				},
				{
					"name": 5,
					"value": 5
				}
			]
		},
		"Deflection_Heading": {
			"type": "heading",
			"label": "Deflection"
		},
		"calculate_deflection": {
			"type": "checkbox",
			"symbol": "Deflection",
			"label": "Calculate Deflection",
			"default": "unchecked",
			"visible_variables": {
				"unchecked": [
					["hide", "E"],
					["hide", "I_z"]
				],
				"checked": [
					["show", "E"],
					["show", "I_z"]
				]
			}
		},
		"E": {
			"type": "number",
			"label": "Young's Modulus (E)",
			"symbol": "E",
			"units": "MPa",
			"info": "Material Young's Modulus",
			"default": 200000,
			"min": 0,
			"step": 1000
		},
		"I_z": {
			"type": "number",
			"label": "Moment of Inertia (I<sub>Z</sub>)",
			"symbol": "I_z",
			"units": "mm^4",
			"info": "Moment of Inertia about Z Axis",
			"default": "5e6",
			"min": 0
		}
	}
}