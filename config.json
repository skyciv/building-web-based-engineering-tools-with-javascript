{
	"meta": {
		"name": "Simply Supported Beam Analysis",
		"short_description": "Simple Supported Beam Analysis Software to calculate reactions, bending, shear and deflection results.",
		"tags": "beam, calculator",
		"category": "analysis",
		"access": "private",
		"default_unit_system": "metric",
		"contact": {
			"name": "George Chard",
			"role": "",
			"email": "George.Chard@skyciv.com",
			"company": "SkyCiv Engineering",
			"logo": "https://skyciv.com/media/logos/logo-pack/SkyCiv_Logo_Dark_Poweredby.png"
		}
	},
	"input_variables": {
		"ui_div": {
			"type": "div",
			"id": "ui-div",
			"qd_center": true, 
			"allow_graphic_report": false
		},
		"Arrangement_Heading": {
			"type": "heading",
			"label": "Arrangement"
		},
		"L": {
			"type": "number",
			"integer": true,
			"label": "Beam Span (L)",
			"symbol": "L",
			"units": "mm",
			"info": "Length of your Beam",
			"default": 1200,
			"min": 10,
			"max": 3000
		},
		"Loading_Heading": {
			"type": "heading",
			"label": "Loading"
		},
		"w": {
			"type": "number",
			"label": "Distributed Load",
			"symbol": "w",
			"units": "kN/m",
			"info": "The uniform distributed load magnitude on your beam",
			"default": 1.25,
			"min": 0,
			"max": 400,
			"step": 1
		},
		"P": {
			"type": "dropdown",
			"label": "Point Load",
			"symbol": "P",
			"units": "kN",
			"info": "Magnitude of the point load",
			"options": [
				{
					"name": "No Force",
					"value": 0
				},
				{
					"name": 1,
					"value": 1
				},
				{
					"name": 2,
					"value": 2,
					"selected": true
				},
				{
					"name": 5,
					"value": 5
				}
			]
		},
		"PL_dist": {
			"type": "number",
			"label": "Point Load Distance (d)",
			"symbol": "PL_{d}",
			"units": "mm",
			"info": "Distance of point load from the start of the member",
			"default": 250,
			"min": 0,
			"step": 1
		},
		"Deflection_Heading": {
			"type": "heading",
			"label": "Deflection"
		},
		"calculate_deflection": {
			"type": "checkbox",
			"symbol": "D_{y}",
			"is_locked": true,
			"label": "Calculate Deflection",
			"default": false,
			"visible_variables": {
				"false": [
					[
						"hide",
						"E"
					],
					[
						"hide",
						"I_z"
					]
				],
				"true": [
					[
						"show",
						"E"
					],
					[
						"show",
						"I_z"
					]
				]
			}
		},
		"E": {
			"type": "number",
			"label": "Young's Modulus (E)",
			"symbol": "E",
			"units": "MPa",
			"info": "Material Young's Modulus",
			"default": 200000,
			"min": 0,
			"step": 1000
		},
		"I_z": {
			"type": "number",
			"label": "Moment of Inertia (I<sub>Z</sub>)",
			"symbol": "I_z",
			"units": "mm^4",
			"info": "Moment of Inertia about Z Axis",
			"default": "2.2e8",
			"min": 0,
			"step": 1000
		}
	}
}